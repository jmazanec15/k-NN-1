{% import "benchmark.helpers" as benchmark with context %}
{
    "name": "msearch",
    "default": false,
    "schedule": [
        {
            "operation": {
                "name": "random-msearch",
                "operation-type": "custom-msearch",
                "index": "{{ target_index_name }}",
                "param-source": "knn-batch-query-from-random",
                "k": {{ query_k }},
                "dimension": {{ target_index_dimension }},
                "field": "{{ target_field_name }}",
                "batch_size": {{ query_batch_size }}
            },
            "clients": {{ query_clients }},
            "iterations": {{ queries_per_client }},
            "warmup-iterations": {{ query_warmup_iterations }},
            "target-throughput": {{ query_target_throughput }}
        }
    ]
}
